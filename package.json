{
  "name": "shopify-node-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prod": "yarn run clean && yarn run build",
    "start-dev": "NODE_ENV=development nodemon ./bin/www",
    "client-only": "parcel ./test/harness.html",
    "start": "node ./bin/www",
    "build": "parcel build ./client/index.js",
    "build-dev": "parcel build ./client/index.js --no-minify",
    "clean": "rm -rf ./dist",
    "pretty": "prettier --single-quote --trailing-comma es5 --write {client,bin,config,server}{/*,/**/*}.js",
    "precommit": "yarn run pretty",
    "postinstall": "yarn run prod",
    "test": "jest"
  },
  "engines": {
    "node": "8.11.3"
  },
  "browsers": [
    "last 3 chrome versions",
    "last 3 firefox versions",
    "last 2 versions",
    "safari >= 8",
    "ios >= 8",
    "ie >= 11",
    "explorermobile >= 11",
    "android >= 4.4"
  ],
  "dependencies": {
    "@shopify/polaris": "2.5.0",
    "@shopify/shopify-express": "^1.0.0-alpha.7",
    "aws-sdk": "^2.289.0",
    "axios": "^0.18.0",
    "chalk": "^1.1.3",
    "config": "^1.30.0",
    "connect-redis": "^3.3.0",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "express-session": "^1.15.3",
    "knex": "^0.13.0",
    "mongoose": "^5.2.7",
    "morgan": "~1.8.1",
    "multer": "^1.3.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-object-inspector": "^0.2.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.3.1",
    "redis": "^2.8.0",
    "redux": "^3.6.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "shopify-api-node": "^2.11.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "3.19.0",
    "eslint-plugin-prettier": "^2.1.2",
    "global": "^4.3.2",
    "jest": "^23.4.0",
    "nodemon": "^1.18.3",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.5.2",
    "react-hot-loader": "^4.3.3"
  }
}
